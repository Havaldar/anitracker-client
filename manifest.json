{
  // Main Info
  "manifest_version": 2,
  "version": "1.0",
  "name": "Anitracker",

  // Desc Info
  "description": "This extension helps you track anime episodes and when they come out!",
  

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "views/popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}

